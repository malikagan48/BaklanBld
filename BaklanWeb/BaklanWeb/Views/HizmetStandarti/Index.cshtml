@model IEnumerable<BaklanWeb.Models.Model.HizmetStandarti>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HizmetStandartiAciklama)
        </th>
        <th></th>
