@model IEnumerable<BaklanWeb.Models.Model.TamamlananProjelerKategori>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Tamamlanan Projeler Kategoriler</h2>

<p>
    @Html.ActionLink("Yeni Oluştur", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TamamlananProjelerKategoriAd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TamamlananProjelerKategoriAciklama)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TamamlananProjelerKategoriAd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TamamlananProjelerKategoriAciklama)
            </td>
            <td>
                @Html.ActionLink("Güncelle", "Edit", new { id = item.TamamlananProjelerKategoriId }) |
                @Html.ActionLink("Detay", "Details", new { id = item.TamamlananProjelerKategoriId }) |
                @Html.ActionLink("Sil", "Delete", new { id = item.TamamlananProjelerKategoriId })
            </td>
        </tr>
    }

</table>
